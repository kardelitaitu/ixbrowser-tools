{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": ["es2022", "dom", "es2015.iterable"],
    "baseUrl": ".",
    "paths": {
      "@licensing/*": ["licensing/*"],
      "@licensing-master/*": ["licensing-master/*"]
    }
  },
  "include": [
    "src",
    "test-licensing.ts",
    "licensing/**/*.ts",
    "licensing-master/**/*.ts",
    "licensing/check-license.js", "licensing/public-key.artifact"  ],
  "exclude": [
    "node_modules"
  ]
}